function c() {
	python script.py compile
}

function s() {
	scp $(for i in $(cat tosend|head -n 10);do echo -n " "$i; done) cs1120233@ssh1.iitd.ac.in:~/private_html/
}

function cs() {
	c;s;
}

function add() {
	tmpfile="/tmp/null";
	echo $1 > $tmpfile;
	cat tosend >> $tmpfile;
	mv $tmpfile tosend
}


#$1 $2
c
